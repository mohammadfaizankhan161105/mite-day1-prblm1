#include <stdio.h>

int main() {
    int a, b, c;
    printf("Enter the first number\n");
    scanf("%d", &a);
    printf("Enter the second number\n");
    scanf("%d", &b);
    printf("Enter the third number\n");
    scanf("%d", &c);

    if (((a + b) / 2.0) > c) {
        printf("YES\n");
    } else {
        printf("NO\n");
    }
